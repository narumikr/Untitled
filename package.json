{
  "name": "@naru/untitled-ui-library",
  "version": "3.0.0",
  "description": "TypeScript components library for React and utility functions",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "style": "dist/color/sekai-colors.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./sekai-style.css": {
      "import": "./dist/esm/sekai-style.css",
      "require": "./dist/cjs/sekai-style.css"
    },
    "./color/sekai-colors.css": {
      "import": "./dist/color/sekai-colors.css",
      "require": "./dist/color/sekai-colors.css"
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "type": "module",
  "scripts": {
    "clean": "rm -rf dist",
    "generate-index": "python scripts/generator.py",
    "build:gen": "npm run generate-index",
    "build:bundle": "rollup -c",
    "build:typecheck": "tsc --noEmit",
    "build:package-json": "python scripts/build-package-json.py",
    "build:local": "sh scripts/build_local.sh",
    "build": "npm run clean && npm run build:gen && npm run build:bundle && npm run build:typecheck && npm run build:package-json && npm run build:local",
    "lint": "npx eslint .  --no-cache --fix --max-warnings=0",
    "prettier": "npx prettier --write . --ignore-path .prettierignore",
    "plop": "npx plop component",
    "dev": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "jest"
  },
  "keywords": [],
  "author": "",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.5",
    "@chromatic-com/storybook": "^5.0.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-onboarding": "10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@svgr/rollup": "^8.1.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.3.2",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.56.0",
    "autoprefixer": "^10.4.21",
    "baseline-browser-mapping": "^2.10.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.1.10",
    "eslint-plugin-unused-imports": "^4.4.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "plop": "^4.0.5",
    "postcss": "^8.5.6",
    "rollup": "^4.55.1",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-tsconfig-paths": "^1.5.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "sass": "^1.96.0",
    "storybook": "^10.2.8",
    "ts-jest": "^29.4.6",
    "typescript": "^5.9.3",
    "typescript-plugin-css-modules": "^5.2.0",
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
